{
    "name": "spryker-sdk/benchmark",
    "type": "library",
    "description": "Benchmark module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "phpbench/phpbench": "^1.0.0",
        "psr/http-message": "^1.0.0",
        "spryker/guzzle": "^2.1.0",
        "spryker/kernel": "^3.54.0",
        "spryker/symfony": "^3.3.2",
        "spryker/util-encoding": "^2.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/config": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerSdk\\": "src/SprykerSdk/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
